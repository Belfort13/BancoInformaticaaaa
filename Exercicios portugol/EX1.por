programa
{
	
	funcao inicio()
	{
		real nota_final, segunda_nota , primeira_nota

		escreva("DIGITE A PRIMEIRA NOTA: ")
		leia(primeira_nota)
		escreva("DIGITE A SEGUNDA NOTA: ")
		leia(segunda_nota)

          nota_final=primeira_nota+segunda_nota
          se(nota_final >= 60)
	{ escreva(nota_final)
	}
	senao
	{ escreva(nota_final)
	escreva("\n REPROVADO")
		
		}
		
	}
}

